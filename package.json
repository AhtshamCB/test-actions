{
  "name": "teefiapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android:dev": "npx react-native run-android --variant=devDebug --appIdSuffix 'dev'",
    "android:staging": "npx react-native run-android --variant=stagingDebug --appIdSuffix 'staging'",
    "android:production": "npx react-native run-android --variant=prodDebug --appIdSuffix 'production'",
    "ios:staging": "npx react-native run-ios --scheme 'TeeFiAppStaging' --configuration 'DebugStaging' --simulator 'iPhone 11 Pro Max'",
    "ios:dev": "npx react-native run-ios --scheme 'TeeFiAppDev' --configuration 'DebugDev' --simulator 'iPhone 15 Pro'",
    "ios:production": "npx react-native run-ios --scheme 'TeeFiAppProd' --configuration 'DebugProd' --simulator 'iPhone 12 mini'",
    "build:apk": "cd ./android && ./gradlew assembleRelease && cd ../",
    "build:aab": "cd ./android && ./gradlew bundleRelease && cd ../",
    "cp:android:staging": "appcenter codepush release-react -a TeeFuture/Teefi-android -d Staging",
    "cp:android:prod": "appcenter codepush release-react -a TeeFuture/Teefi-android -d Production",
    "cp:ios:staging": "appcenter codepush release-react -a TeeFuture/Teefi-IOS -d Staging",
    "cp:ios:prod": "appcenter codepush release-react -a TeeFuture/Teefi-IOS -d Production"
  },
